#!/bin/fish
for arg in $argv
for var in (seq 2 25)
  echo -n $arg' % '$var' = '
  set mod (math $arg % $var)
  echo -n $mod' '
  [ $mod -eq 0 ] && echo -n ja
  [ $mod -eq 1 ] && echo -n Gegenteil
  [ $mod -eq 2 ] && echo -n 채hnlich
  [ $mod -eq 3 ] && echo -n entferntes Gegenteil
  [ $mod -eq 4 ] && echo -n entfernt 채hnlich
  set mod (math $var - $mod)
  echo -n ', '
  [ $mod -eq 0 ] && echo -n ja
  [ $mod -eq 1 ] && echo -n Gegenteil
  [ $mod -eq 2 ] && echo -n 채hnlich
  [ $mod -eq 3 ] && echo -n entferntes Gegenteil
  [ $mod -eq 4 ] && echo -n entfernt 채hnlich
  echo
end
echo
end
